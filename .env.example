# Environment Variables for Fraudlr
# Copy this file to .env and fill in your values

# Database Connection
# PostgreSQL connection string
DATABASE_URL="postgresql://username:password@localhost:5432/fraudlr"

# Authentication
# JWT secret for signing tokens (use a strong random string, at least 32 characters)
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="7d"

# Application URLs
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"

# Optional: AI/ML API Configuration (if using external AI service)
AI_API_KEY=""
AI_API_URL=""
